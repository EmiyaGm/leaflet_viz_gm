{
    "rules": {
        "camelcase": 2,
        "quotes": [2, "single"],
        "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
        "space-in-parens": 2,
        "space-before-blocks": 2,
        "space-after-keywords": 2,
        "no-lonely-if": 2,
        "comma-style": 2,
        "no-underscore-dangle": 0,
        "no-constant-condition": 0,
        "no-multi-spaces": 0,
        "strict": 0,
        "key-spacing": 0,
        "no-console": 0,
        "no-shadow": 0,
        "indent": [2, 4],
        "linebreak-style": [2, "windows"],
        "semi": [2, "always"],
        "curly": 2
    },
    "env": {
        "browser": true
    },
    "globals": {
        "L": true
    },
    "extends": "eslint:recommended"
}
